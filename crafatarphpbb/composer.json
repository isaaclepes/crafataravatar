{
    "name": "crafataravatar/crafatarphpbb",
    "type": "phpbb-extension",
    "description": "This extension enables the use of Minecraft avatars via crafatar.com API. This extension supports 3 different skin types. Head 2d, head 3d, body 3d. The admin can also enable or disable the option on registration page.",
    "homepage": "http://www.sitesplat.com/",
    "version": "2.1.0",
    "time": "2020-12-15",
    "keywords": [
        "phpbb",
        "extension",
        "minecraft",
        "avatar",
        "cosmetic",
        "crafatar"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "ThemeSplat (SiteSplat)",
            "email": "info@sitesplat.com",
            "homepage": "http://www.sitesplat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Minecraft Avatar Extension - crafatar.com API",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
    }
}